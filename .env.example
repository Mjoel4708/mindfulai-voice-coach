# Environment variables for the AI Mental Wellness Coach
# Copy this file to .env and fill in your values

# Google Cloud / Vertex AI
GOOGLE_CLOUD_PROJECT=your-gcp-project-id
VERTEX_AI_LOCATION=us-central1

# ElevenLabs
ELEVENLABS_API_KEY=your-elevenlabs-api-key

# Confluent Kafka
KAFKA_BOOTSTRAP_SERVERS=your-cluster.us-central1.gcp.confluent.cloud:9092
KAFKA_API_KEY=your-kafka-api-key
KAFKA_API_SECRET=your-kafka-api-secret

# Database (for local development, use docker-compose default)
DATABASE_URL=postgresql+asyncpg://postgres:postgres@localhost:5432/wellness_coach
